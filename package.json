{
  "name": "dashboard",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://neglux.github.io/banking-app-dashboard",
  "dependencies": {
    "@mantine/core": "^6.0.13",
    "@mantine/hooks": "^6.0.13",
    "@sentry/react": "^7.54.0",
    "dayjs": "^1.11.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.44.3",
    "react-icons": "^4.4.0",
    "react-router-dom": "^6.11.2",
    "react-scripts": "5.0.1",
    "react-toastify": "^9.1.3"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "eslint:recommended"
    ],
    "rules": {
      "react/style-prop-object": 0,
      "import/no-anonymous-default-export": [
        "error",
        {
          "allowArray": true,
          "allowObject": true
        }
      ],
      "no-unused-vars": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.7",
    "gh-pages": "^5.0.0",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.4"
  }
}
